{
  "name": "todo-express",
  "version": "1.0.0",
  "description": "Todo app with Electron and Express",
  "main": "app.js",
  "scripts": {
    "start": "concurrently \"node app.js\" \"electron .\"",
    "build": "electron-builder"
  },
  "dependencies": {
    "bson": "^4.5.0",
    "concurrently": "^6.2.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "path": "^0.12.7",
    "realm": "^10.0.0",
    "realm-web": "^1.0.0",
    "winston": "^3.13.1"
  },
  "devDependencies": {
    "electron": "^13.1.7",
    "electron-builder": "^22.11.7"
  },
  "build": {
    "appId": "com.yourapp.id",
    "files": [
      "**/*",
      "node_modules/realm/**/*"
    ],
    "extraResources": [
      {
        "from": "node_modules/realm",
        "to": "node_modules/realm",
        "filter": [
          "**/*"
        ]
      }
    ]
  }
}
